{
	"name": "gp-contacts-organizer",
	"description": "Web app for managing/organizing Nextcloud contacts.",
	"private": true,
	"version": "0.1.1",
	"type": "module",
	"scripts": {
		"start": "bun --bun ./index.js",
		"dev": "bun run --bun vite dev --host=0.0.0.0 --port=8080 --strictPort",
		"incr_version": "npm version patch --no-git-tag-version",
		"build:vite": "bun --bun run vite build",
		"build": "bun run build:vite",
		"preview": "bun run --bun vite preview --host=0.0.0.0"
	},
	"devDependencies": {
		"@auth/sveltekit": "^1.7.4",
		"@fontsource/ubuntu": "^5.1.1",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@types/bootstrap": "^5.2.10",
		"@types/bun": "^1.1.17",
		"@types/luxon": "^3.4.2",
		"@types/xml2js": "^0.4.14",
		"bootstrap": "^5.3.3",
		"bun-sqlite-key-value": "^1.14.1-beta.1",
		"http-status": "^2.1.0",
		"luxon": "^3.5.0",
		"runed": "^0.23.1",
		"sass": "^1.83.4",
		"svelte": "^5.19.0",
		"svelte-adapter-bun": "^0.5.2",
		"svelte-check": "^4.1.4",
		"sveltekit-superforms": "^2.22.1",
		"tsdav": "^2.1.3",
		"typescript": "^5.7.3",
		"vcard4-tsm": "^1.0.0",
		"vite": "^5.4.11",
		"xml2js": "^0.6.2",
		"zod": "^3.24.1"
	}
